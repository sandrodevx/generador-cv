# Configuración de la aplicación
VITE_APP_NAME=Generador de Currículum Profesional
VITE_PDF_QUALITY=2
VITE_MAX_UPLOAD_SIZE=1048576

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_auth_domain.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_storage_bucket.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# OpenAI Configuration (para IA)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Analytics (opcional)
VITE_GOOGLE_ANALYTICS_ID=your_ga_id

# Feature Flags
VITE_ENABLE_AI_ASSISTANT=true
VITE_ENABLE_CLOUD_SAVE=true
VITE_ENABLE_ANALYTICS=true 